(jbuild_version 1)


(library
 ((name analysisTestSetup)
  (public_name pyrelib.testSetup)
  (modules (analysisTestSetup))
  (libraries 
   (oUnit
    pyrelib.test
    pyrelib.ast
    pyrelib.analysis))))


